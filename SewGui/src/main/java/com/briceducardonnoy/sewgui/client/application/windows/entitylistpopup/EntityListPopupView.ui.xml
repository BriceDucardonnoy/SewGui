<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
			xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
			xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
			ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
			ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
			ui:generateLocales='default'>
    

	<g:PopupPanel ui:field="main" modal="true" glassEnabled="true" autoHideEnabled="true">
	    <b:Alert type="INFO" ui:field="alert">
			<b.html:Strong ui:field="title" text="List of recorded devices"/>
			<b.html:Br />
			<!-- <b.html:Text text="..."/> -->
			<!-- <g:Label>Patate</g:Label> -->
			<b:Button type="PRIMARY" width="100%">Hello</b:Button>
		</b:Alert>
    </g:PopupPanel>
</ui:UiBinder>